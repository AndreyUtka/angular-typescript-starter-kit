{
  "name": "angular-typescript-starter-kit",
  "version": "1.0.0",
  "description": "draft of starter kit which based on webpack/es6/typescript/angular1.x",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreyUtka/angular-typescript-starter-kit.git"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "rimraf typings && typings install --no-insight",
    "start": "webpack-dev-server --inline --colors --display-error-details --display-cached",
    "clean": "rimraf app/__build",
    "build": "npm run clean && webpack",
    "test": "karma start",
    "e2e": "npm run build && gulp e2e"
  },
  "bugs": {
    "url": "https://github.com/AndreyUtka/angular-typescript-starter-kit/issues"
  },
  "homepage": "https://github.com/AndreyUtka/angular-typescript-starter-kit#readme",
  "dependencies": {
    "angular": "^1.5.3",
    "angular-mocks": "^1.5.3",
    "awesome-typescript-loader": "^0.17.0-rc.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "tsd": "^0.6.5",
    "tslint": "^3.6.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.9",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
