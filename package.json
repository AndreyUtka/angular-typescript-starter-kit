{
  "name": "angular-typescript-starter-kit",
  "version": "1.0.0",
  "description": "draft of starter kit which based on webpack/es6/typescript/angular1.x",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreyUtka/angular-typescript-starter-kit.git"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run typings",
    "typings": "rimraf app/__typings && tsd install && tsd link",
    "start": "webpack-dev-server --inline --colors --display-error-details --display-cached",
    "clean": "rimraf app/__build",
    "build": "npm run clean && webpack",
    "test": "karma start",
    "e2e": "npm run build && gulp e2e"
  },
  "bugs": {
    "url": "https://github.com/AndreyUtka/angular-typescript-starter-kit/issues"
  },
  "homepage": "https://github.com/AndreyUtka/angular-typescript-starter-kit#readme",
  "dependencies": {
    "angular": "^1.4.8"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "html-webpack-plugin": "^1.7.0",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.7.1",
    "tslint": "^3.0.0",
    "tslint-loader": "^2.0.0",
    "typescript": "^1.6.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
