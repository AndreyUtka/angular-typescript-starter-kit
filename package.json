{
  "name": "angular-typescript-starter-kit",
  "version": "1.0.0",
  "description": "draft of starter kit which based on webpack/es6/typescript/angular1.x",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreyUtka/angular-typescript-starter-kit.git"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run build-configs",
    "dev-build-configs": "npm run build-configs -- --watch",
    "build-configs": "tsc --project tsconfig.build-configs.json",
    "start": "webpack-dev-server",
    "clean": "rimraf app/__build",
    "build": "npm run clean && webpack -p --config webpack.config.js",
    "test": "karma start"
  },
  "bugs": {
    "url": "https://github.com/AndreyUtka/angular-typescript-starter-kit/issues"
  },
  "homepage": "https://github.com/AndreyUtka/angular-typescript-starter-kit#readme",
  "dependencies": {
    "@types/angular": "1.5.16",
    "@types/angular-mocks": "1.5.5",
    "@types/jasmine": "2.2.34",
    "@types/jquery": "2.0.32",
    "@types/node": "6.0.41",
    "@types/webpack": "1.12.34",
    "angular": "1.5.8",
    "angular-mocks": "1.5.8",
    "awesome-typescript-loader": "2.2.4",
    "css-loader": "0.25.0",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.12",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  }
}
